        function a() {
            var span1 = document.getElementById("span1").innerHTML;
            var span2 = document.getElementById("span2").innerHTML;
            var span3 = document.getElementById("span3").innerHTML;
            var span4 = document.getElementById("span4").innerHTML;
            var span5 = document.getElementById("span5").innerHTML;
            if (span1 == 0) {
                document.getElementById("span1").style.display = "none";
            }
            if (span2 == 0) {
                document.getElementById("span2").style.display = "none";
            }
            if (span3 == 0) {
                document.getElementById("span3").style.display = "none";
            }
            if (span4 == 0) {
                document.getElementById("span4").style.display = "none";
            }
            if (span5 == 0) {
                document.getElementById("span5").style.display = "none";
            }
        }

        function ShowDivCK(titletype, a, b, c, d) {
            if (titletype == "1") {
                $("#title_home").text("待派遣事件");
                $("#content_ifram").attr("src", "/HomeSystem/modify?ID=" + a + "&WFSAID=" + b + "&WFSID=" + c + "&WFDID=" + d + "");
            }
            else if (titletype == "2") {
                $("#title_home").text("待审核事件");
                $("#content_ifram").attr("src", "/HomeSystem/Check?ID=" + a + "&WFSAID=" + b + "&WFSID=" + c + "&WFDID=" + d + "");
            }
            else if (titletype == "3") {
                $("#title_home").text("挂起事件");
                $("#content_ifram").attr("src", "/HomeSystem/DealWith?ID=" + a + "&WFSAID=" + b + "&WFSID=" + c + "&WFDID=" + d + "");
            }
            else if (titletype == "4") {
                $("#title_home").text("事件详情");
                $("#content_ifram").attr("src", "/HomeSystem/GetView?ID=" + a + "&WFSAID=" + b + "&WFSID=" + c + "&WFDID=" + d + "");
            }
            else if (titletype == "5") {
                $("#title_home").text("事件派遣");
                $("#content_ifram").attr("src", "/HomeSystem/ReportDispatch?ID=" + a + "");
            } else if (titletype == "6") {
                $("#title_home").text("公告");
                $("#content_ifram").attr("src", "/HomeSystem/Look?NoticeId=" + a + "");
            }
            document.getElementById("MyDiv").style.display = 'block';
            document.getElementById("fade").style.display = 'block';
            var bgdiv = document.getElementById("fade");
            bgdiv.style.width = document.body.scrollWidth;
            $("#fade").height($(document).height());
            $("#MyDiv").css("top", ($(document).scrollTop() + 80));

        }
        //关闭弹出层
