        //带派遣事件列表
        function UpcomingEventsTableList() {
            $.ajax({
                url: "/HomeSystem/UpcomingEventsTableList",
                type: "POST",
                cache: false,
                success: function (data) {
                    if (data.length == 0) {
                        var html = "";
                        html += "<tr>";
                        html += "<td colspan='5'>没有数据</td>"
                        html += "</tr>";
                        $(ZFSJWorkFlowTable.ID).append(html);
                    } else {
                        var html = "";
                        $.each(data, function (i, t) {
                            html += "<tr>";

                            html += "<td>" + t.EVENTCODE + "</td>";
                            if (t.EVENTTITLE.length < 45) {
                                html += "<td style='text-align: left;'title='" + t.EVENTTITLE + "'>" + t.EVENTTITLE + "</td>";
                            } else {
                                html += "<td style='text-align: left;'title='" + t.EVENTTITLE + "'>" + t.EVENTTITLE.substring(0, 42) + "..." + "</td>";
                            }

                            html += "<td>" + t.SOURCENAME + "</td>";
                            html += "<td>" + t.createtime + "</td>";
                            if (t.username == null || t.username == "") {
                                html += "<td>无</td>";
                            } else {
                                html += "<td>" + t.username + "</td>";
                            }

                            if (t.judgment == 1) {
                                html += '<td><a style="color:#08c" href="javascript:void(0);" onclick="ShowDivCK(\'' + 5 + '\',\'' + t.ZFSJID + '\')" >处理</a></td>';
                            } else {
                                html += '<td><a style="color:#08c" href="javascript:void(0);" onclick="ShowDivCK(\'' + 1 + '\',\'' + t.ZFSJID + '\',\'' + t.wfsaid + '\',\'' + t.wfsid + '\',\'' + t.wfdid + '\')" >处理</a></td>';
                            }

                            //href = "/XTGL/modify?ID=' + t.ZFSJID + '&WFSAID=' + t.wfsaid + '&WFSID=' + t.wfsid + '&WFDID=' + t.wfdid + '"
                            html += "</tr>";
                        });
                        $(ZFSJWorkFlowTable.ID).append(html);
                    }
                }
            });

        }
