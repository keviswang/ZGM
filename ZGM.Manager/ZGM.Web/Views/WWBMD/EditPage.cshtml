                                <div class="col-sm-9 col-lg-10 controls">
                                    @Html.TextBoxFor(t => Model.REWARD, new { @class = "large", @style = "width: 98%" })
                                </div>
                            </div>
                        </td>
                    </tr>

                    <tr>
                        <th style="text-align: center; line-height: 20px;" rowspan="2">社区矫正期限 </th>
