                            </div>
                        </td>
                    </tr>

                    <tr>
                        <th style="text-align: center; line-height: 20px;">姓名
                        </th>
                        <td style="width: 15%">
                            <div class="form-group">
                                <div class="col-sm-9 col-lg-10 controls">
                                    @Html.TextBoxFor(t => Model.NAME, new { @class = "large" })
