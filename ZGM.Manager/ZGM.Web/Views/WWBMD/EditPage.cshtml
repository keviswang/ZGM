                                    {
                                        <span class='mapspanstyle'><a class='MapClass' style='margin-right:10px; font-size:14px;' value='" + @item.GEOMETRY + "' onclick='javascript:mapInited(this)' GEOMETRY='@item.GEOMETRY' ADDRESSNAME=' @item.ADDRESSNAME '>@item.ADDRESSNAME </a><a onclick=Remove(this);>删除</a></span>
                                    }
                                </label>
                            </div>

                            <div class="form-group" id="ShowOrHide" style="margin-bottom: 5px;">
                                <div class="col-sm-9 col-lg-10 controls">
                                </div>
                                <input type="hidden" id="GEOMETRY" />
