                            <div class="form-group">
                                <div class="col-sm-9 col-lg-10 controls">
                                    <input type="text" name="WJUNIT" class="large" />
                                </div>
                            </div>
                        </td>
                        <th style="text-align: center; line-height: 30px;width: 10%;">所在片区 
                        </th>
                        <td style="line-height: 30px; width: 40%">
                            <div class="form-group">
                                <div class="col-sm-9 col-lg-10 controls">
                                    @Html.DropDownList("ZONEID", ViewBag.zones as IEnumerable<SelectListItem>, "请选择", new {@style = "width:98%;" })
                                </div>
                            </div>
                        </td>
                    </tr>


                    <tr>
                        <th style="text-align: center; line-height: 30px">违建地点
                        </th>
                        <td style="line-height: 30px; width: 40%">
                            <div class="form-group" style="margin-bottom: 5px;">
                                <div class="col-sm-9 col-lg-10 controls">
                                    <input type="text" name="WJADDRESS" class="large" />
                                </div>
                            </div>
                        </td>
                        <th style="text-align: center; line-height: 30px">当前状态
                        </th>
                        <td style="line-height: 30px; width: 40%">
                            <div class="form-group" style="margin-bottom: 5px;">
                                <div class="col-sm-9 col-lg-10 controls">
                                    <select class="select2" id="STATE" name="STATE" style="width: 98%;">
                                        <option value="">请选择</option>
                                        <option value="1">已拆</option>
                                        <option value="2">未拆</option>
                                    </select>
                                </div>
                            </div>
                        </td>
                    </tr>



                    <tr>
                        <th style="text-align: center; line-height: 30px;">个人或法人代表身份
                        </th>
                        <td>
                            <div class="form-group">
                                <div class="col-sm-9 col-lg-10 controls">
                                    <select class="select2" id="IDENTITYID" name="IDENTITYID" style="width: 98%;">
                                        <option value="">请选择</option>
                                        <option value="1">机关</option>
                                        <option value="2">事业</option>
                                        <option value="3">国企工作人员</option>
                                        <option value="4">中共党员</option>
                                        <option value="5">各级人大代表</option>
                                        <option value="6">政协委员</option>
                                        <option value="7">村两委干部</option>
                                        <option value="8">其它</option>
                                    </select>
                                </div>
                            </div>
                        </td>
                        <th style="text-align: center; line-height: 30px;">联系电话
                        </th>
                        <td style="line-height: 30px; width: 40%">
                            <div class="form-group">
                                <div class="col-sm-9 col-lg-10 controls">
                                    <input type="text" name="TEL" class="large" />
                                </div>
                            </div>
                        </td>
                    </tr>

                    <tr>
                        <th style="text-align: center; line-height: 30px">地图位置
                        </th>
                        <td colspan="3">
                            <input id="showmap" type="button" value="显示地图" onclick="GetShowOrHide(this, 'show')" />
                            <div class="form-group" id="ShowOrHide" style="margin-bottom: 5px; display: none">
                                <div class="col-sm-9 col-lg-10 controls">
                                </div>
                                @Html.HiddenFor(model => model.MAPLOACTION, new { @name = "MAPLOACTION", @id = "MAPLOACTION" })
                                <div id="silverlightControlHost" style="width: 100%; height: 350px;">
                                    <object id="mapCtrl" data="data:application/x-silverlight-2," type="application/x-silverlight-2" height="350px" width="100%">
                                        <param name="source" value="/ClientBin/MapCtrl.xap" />
                                        <param name="background" value="white" />
