        //后台回调js函数
        function AddCallBack(flag) {
            $("#silverlightControlHost").css("visibility", "hidden");
            $(parent.frames["leftFrame"].document).find(".leftmenu dd:eq(0) li").prop("class", "");
            $(parent.frames["leftFrame"].document).find(".leftmenu dd:eq(1) li").prop("class", "active");
            if (flag == 1) {
                layer.alert("操作成功!", {
                    icon: 1,
                    skin: 'layer-ext-seaning'
                }, function (index) {
                    layer.close(index);
                    location.href = "/WJGL/List";
                });
            }
            if (flag == 2) {
                layer.alert("操作失败!", {
                    icon: 0,
                    skin: 'layer-ext-seaning'
                }, function (index) {
                    layer.close(index);
                    location.href = "/WJGL/List";
                });
            }
        }

        function ClearFile(fileid, imgid) {
            if ($("#" + imgid).attr("src") != "/Images/20076110836311.png" && $("#hidden-save").css("display") == "block") {
                layer.confirm('是否确认删除?', {
                    btn: ['是', '否'],
                    icon: 4,
                    //按钮
                }, function () {
                    document.getElementById(fileid).outerHTML += '';//清空IE的
                    document.getElementById(fileid).value = "";//可以清空火狐的
                    var strpath = $("#" + imgid).attr("src").replace("/GetPictureFile.ashx?PicPath=C:\\ZGMImage\\WFJZFilesPath\\sourse\\", "");
                    $.ajax({
                        type: "GET",
                        url: "/WJGL/ClearFileImage?ClearFilePath=" + strpath,
                        success: function (data) {
                            $("#" + imgid).attr("src", "/Images/20076110836311.png");
                            layer.alert("删除成功");
                        }
                    })
                }, function () {
                    layer.close();
                });
            }
        }

        function ClearFile2(fileid, imgid) {
            document.getElementById(fileid).outerHTML += '';//清空IE的
            document.getElementById(fileid).value = "";//可以清空火狐的
            $("#" + imgid).attr("src", "/Images/20076110836311.png");
        }

    </script>
    <script type="text/javascript">
        $(document).ready(function () {
            var WJID = getQueryString("WJID");
            $("input[name='hidden-wjid']").val(WJID);
            if (WJID != null) {
                $(".placeul li:eq(0)").text("违法建筑详情");
                $(".placeul li:eq(0)").before("<li><a href='/WJGL/List'>违法建筑列表</a></li>");
                $("#hidden-edit,#hidden-remark,#hidden-afterpic,#backbtn").show();
                $("table input,select,textarea").attr("disabled", "disabled");
                $("table input,select,textarea").css("cursor", "not-allowed");
                $("#showmap").removeAttr("disabled").css("background", "white").css("cursor", "pointer");
                $("#hidden-beforepic .delete").prop("class", "delete1");
                $("#hidden-beforepic .delete1,#beforepicupload").hide();
                $.ajax({
                    type: "GET",
                    url: "/WJGL/GetWJGLByWJID?random=" + Math.random(),
                    data: {
                        WJID: WJID
                    },
                    success: function (data) {

                        $("input[name='WJUNIT']").val(data.list.WJUNIT);
                        $("#ZONEID").val(data.list.ZONEID).prop("selected", true);
                        $("input[name='WJADDRESS']").val(data.list.WJADDRESS);
                        $("#STATE").val(data.list.STATE).prop("selected", true);
                        $("#IDENTITYID").val(data.list.IDENTITYID).prop("selected", true);
                        $("input[name='TEL']").val(data.list.TEL);
                        $("#MAPLOACTION").val(data.list.MAPLOACTION);
                        $("input[name='WJTIME']").val(data.list.WJTIMEStr);
                        $("#WJTYPE").val(data.list.WJTYPE).prop("selected", true);
                        $("#WJFRAME").val(data.list.WJFRAME).prop("selected", true);
                        $("input[name='WJFLOOR']").val(data.list.WJFLOOR);
                        $("#LANDTYPE").val(data.list.LANDTYPE).prop("selected", true);
                        $("#WJUSE").val(data.list.WJUSE).prop("selected", true);
                        $("input[name='LANDAREA']").val(data.list.LANDAREA);
                        $("input[name='BUILDINGAREA']").val(data.list.BUILDINGAREA);
                        $("input[name='REMOVETIME']").val(data.list.REMOVETIMEStr);
                        $("input[name='REMOVEAREA']").val(data.list.REMOVEAREA);
                        $("input[name='CONSTRUCTIONPROJECT']").val(data.list.CONSTRUCTIONPROJECT);
                        $("#SIXCASE").val(data.list.SIXCASE).prop("selected", true);
                        $("#RECTIFICATIONCASE").val(data.list.RECTIFICATIONCASE).prop("selected", true);
                        $("input[name='RECTIFICATIONTIME']").val(data.list.RECTIFICATIONTIMEStr);
                        $("textarea[name='REMARK1']").val(data.list.REMARK1);
                        $("#beforepicshow td").css("border", "none");

                        if (data.list.BeforePic != null) {
                            var bpicarr = data.list.BeforePic.split("|");
                            for (var i = 0; i < bpicarr.length; i++) {
                                $("#img" + (i + 1)).attr("src", bpicarr[i]);
                            }
                            for (var i = 0; i <= $("img[id^='img']").length; i++) {
                                if ($("#img" + i).attr("src") == "/Images/20076110836311.png")
                                    $("#hidden-beforepic #img" + i).parent().parent().hide();
                            }
                        } else {
                            $("#beforepicshow").text("未上传改造前的图片");
                            $("#beforepicshow").css("color", "#999").css("font-size", "14px");
                        }

                        if (data.list.AfterPic != null) {
                            var apicarr = data.list.AfterPic.split("|");
                            for (var i = 0; i < apicarr.length; i++) {
                                $("#img" + (i + 7)).attr("src", apicarr[i]);
                            }
                        }

                    }
                })

            } else
                $("#addbtn").show();

            $("input[name='WJTIME']").datepicker({ changeMonth: true, changeYear: true, showMonthAfterYear: true, showButtonPanel: true });
            $("input[name='REMOVETIME']").datepicker({ changeMonth: true, changeYear: true, showMonthAfterYear: true, showButtonPanel: true });
            $("input[name='RECTIFICATIONTIME']").datepicker({ changeMonth: true, changeYear: true, showMonthAfterYear: true, showButtonPanel: true });

            //违法建筑编辑
            $("#hidden-edit").click(function () {
                $("#hidden-edit").hide();
                $("#hidden-save").show();
                $("#STATE,input[name='REMOVETIME'],input[name='REMOVEAREA'],input[name='CONSTRUCTIONPROJECT'],#RECTIFICATIONCASE," +
                "input[name='RECTIFICATIONTIME'],#hidden-afterpic input,textarea[name='REMARK1']").removeAttr("disabled").css("cursor", "unset");
            })
        });
        function validate_form(thisform) {
            var PHONE = $("input[name='TEL']").val();
            var allcount = $("#hidden-afterpic img").length;
            var count = 0;
            for (var i = 0; i < allcount; i++) {
                if ($("#hidden-afterpic img:eq(" + i + ")").attr("src") == "/Images/20076110836311.png")
                    count++;
            }
            $("input[name='hidden-aftercount']").val(6 - count);
            if ($("#hidden-save").css("display") == "block")
                $("input[name='hidden-isedit']").val(1);
            if (PHONE == null || PHONE == "") {
                $("#silverlightControlHost").css("visibility", "hidden");
                layer.alert("联系电话不能为空!", {
                    icon: 0,
                    skin: 'layer-ext-seaning'
                }, function (index) {
                    layer.close(index);
                    $("#silverlightControlHost").css("visibility", "visible");
                });
                return false;
            }
            if (PHONE != null && PHONE != "" && !(/^[1][358][0-9]{9}$/.test(PHONE)) && !(/^0\d{2,3}-?\d{7,8}$/.test(PHONE))) {
                $("#silverlightControlHost").css("visibility", "hidden");
                layer.alert("联系电话格式不正确!", {
                    icon: 0,
                    skin: 'layer-ext-seaning'
                }, function (index) {
                    layer.close(index);
                    $("#silverlightControlHost").css("visibility", "visible");
                });
                MAPLOACTION.focus();
                return false;
            }

            with (thisform) {
                if (validate_required(WJUNIT, "违建单位不能为空!") == false)
                { WJUNIT.focus(); return false }
                if (validate_required(ZONEID, "所在片区不能为空!") == false)
                { ZONEID.focus(); return false }
                if (validate_required(WJADDRESS, "违建地点不能为空!") == false)
                { WJADDRESS.focus(); return false }
                if (validate_required(STATE, "当前状态不能为空!") == false)
                { STATE.focus(); return false }
                if (validate_required(IDENTITYID, "个人或法人代表身份不能为空!") == false)
                { IDENTITYID.focus(); return false }
                if (validate_required(MAPLOACTION, "地图位置不能为空!") == false)
                { MAPLOACTION.focus(); return false }
                if (validate_required(WJTYPE, "违建类别不能为空!") == false)
                { WJTYPE.focus(); return false }
                if (validate_required(WJFRAME, "违建结构不能为空!") == false)
                { WJFRAME.focus(); return false }
                if (validate_required_float(WJFLOOR, "违建楼层不能为空!", "违建楼层格式不正确!") == false)
                { WJFLOOR.focus(); return false }
                if (validate_required(LANDTYPE, "土地类别不能为空!") == false)
                { LANDTYPE.focus(); return false }
                if (validate_required(WJUSE, "违建用途不能为空!") == false)
                { WJUSE.focus(); return false }
                if (validate_required_float(LANDAREA, "占地面积不能为空!", "占地面积格式不正确!") == false)
                { LANDAREA.focus(); return false }
                if (validate_required_float(BUILDINGAREA, "建筑面积不能为空!", "建筑面积格式不正确!") == false)
                { BUILDINGAREA.focus(); return false }
                if (validate_required_float_null(REMOVEAREA, "拆除面积不能为空!", "拆除面积格式不正确!") == false)
                { REMOVEAREA.focus(); return false }
                if (validate_required(SIXCASE, "六类先拆情况不能为空!") == false)
                { SIXCASE.focus(); return false }
                if (validate_required(RECTIFICATIONCASE, "整改通知发放情况不能为空!") == false)
                { RECTIFICATIONCASE.focus(); return false }
                else { $("table input,select,textarea").removeAttr("disabled"); return true }
            }
        }

        function validate_required(field, alerttxt) {
            with (field) {
                if (value == null || value == "") {
                    $("#silverlightControlHost").css("visibility", "hidden");
                    layer.alert(alerttxt, {
                        icon: 0,
                        skin: 'layer-ext-seaning'
                    }, function (index) {
                        layer.close(index);
                        $("#silverlightControlHost").css("visibility", "visible");
                    });
                    return false
                }
                else { return true }
            }
        }

        //为空或者为数字
        function validate_required_float_null(field, alertnulltxt, alerterrortxt) {
            var intregex = /^[0-9]+(.[0-9]{1,5})?$/;
            with (field) {
                if (value != null && value != "" && !intregex.test(value)) {
                    $("#silverlightControlHost").css("visibility", "hidden");
                    layer.alert(alerterrortxt, {
                        icon: 0,
                        skin: 'layer-ext-seaning'
                    }, function (index) {
                        layer.close(index);
                        $("#silverlightControlHost").css("visibility", "visible");
                    });
                    return false
                }
                return true;
            }
        }

        //验证是否为空并且为数字
        function validate_required_float(field, alertnulltxt, alerterrortxt) {
            var intregex = /^[0-9]+(.[0-9]{1,5})?$/;
            with (field) {
                if (value == null || value == "") {
                    $("#silverlightControlHost").css("visibility", "hidden");
                    layer.alert(alertnulltxt, {
                        icon: 0,
                        skin: 'layer-ext-seaning'
                    }, function (index) {
                        layer.close(index);
                        $("#silverlightControlHost").css("visibility", "visible");
                    });
                    return false
                }
                if (value != null && value != "" && !intregex.test(value)) {
                    $("#silverlightControlHost").css("visibility", "hidden");
                    layer.alert(alerterrortxt, {
                        icon: 0,
                        skin: 'layer-ext-seaning'
                    }, function (index) {
                        layer.close(index);
                        $("#silverlightControlHost").css("visibility", "visible");
                    });
                    return false
                }
                if (value != null && value != "" && intregex.test(value))
                { return true }
            }
        }

        function IndexStr(str2) {
            var str1 = "请选择";
            var s = str1.indexOf(str2);
            return (s);
        }

        //地图描点
        var geometry = "";
        function mapInited() {
            setTimeout(function () {
                var DTMD = $("input[name=MAPLOACTION]")[0].value;
                if (DTMD != "") {
                    var mapCtrl = document.getElementById("mapCtrl").content.MapCtrl;
                    mapCtrl.AddMarker(DTMD);
                }
            }, 1000)
        }

        //地图坐标点
        function mapDraw(s) {
            geometry = s;
            document.getElementById("MAPLOACTION").value = geometry;
        }
        $("#confirm").click(function () {
            document.getElementById("MAPLOACTION").value = geometry;
            $("#mapDepicting").modal('hide');
        });

        //显示隐藏地图
        function GetShowOrHide(o, otype) {
            if (otype == "show") {
                o.value = "隐藏地图";
                o.setAttribute("onclick", "GetShowOrHide(this,'hide')");
                document.getElementById("ShowOrHide").style.display = 'block';

            } else if (otype == 'hide') {
                o.value = "显示地图";
                o.setAttribute("onclick", "GetShowOrHide(this,'show')");
                document.getElementById("ShowOrHide").style.display = 'none';
            }
        }

        //图片上传
        function fileUpload(fileid, imgid) {
            var img_count = parseInt(fileid.length == 10 ? fileid.substr(-1, 1) : fileid.substr(-2, 2));
            if (img_count > 6 && $(imgid).attr("src") != "/Images/20076110836311.png") {
                if ($("input[name='hidden-image']").val() == 0)
                    $("input[name='hidden-image']").val(img_count - 6);
                else
                    $("input[name='hidden-image']").val($("input[name='hidden-image']").val() + "," + (img_count - 6));
            }

            var $file = $(fileid);
            var fileObj = $file[0];
            var windowURL = window.URL || window.webkitURL;
            var dataURL;
            var $img = $(imgid);

            if (fileObj && fileObj.files && fileObj.files[0]) {
                dataURL = windowURL.createObjectURL(fileObj.files[0]);
                $img.attr('src', dataURL);
            } else {
                dataURL = $file.val();
                var imgObj = $(imgid);
                // 两个坑:
                // 1、在设置filter属性时，元素必须已经存在在DOM树中，动态创建的Node，也需要在设置属性前加入到DOM中，先设置属性在加入，无效；
                // 2、src属性需要像下面的方式添加，上面的两种方式添加，无效；
                imgObj.style.filter = "progid:DXImageTransform.Microsoft.AlphaImageLoader(sizingMethod=scale)";
                imgObj.filters.item("DXImageTransform.Microsoft.AlphaImageLoader").src = dataURL;
            }
        }

        //获取url参数
        function getQueryString(name) {
            var reg = new RegExp("(^|&)" + name + "=([^&]*)(&|$)", "i");
            var r = window.location.search.substr(1).match(reg);
            if (r != null)
                return unescape(r[2]);
            return null;
        }
    </script>
</head>

<body>
    <div class="place" style="background-image: url(/Images/images/righttop.gif)">
        <span>位置：</span>
        <ul class="placeul">
            <li>新建违法建筑</li>
        </ul>
    </div>
    <div class="formbody">

        <form method="post" enctype="multipart/form-data" action="/WJGL/Commit" target="hidden_frame" onsubmit="return validate_form(this)" id="EventReport">
            <div>
                <div>
                    <h6 style="font-size: 32px; text-align: center;margin-top:10px;margin-bottom:20px;">违法建筑基本信息</h6>
                    <input id="hidden-edit" style="float: right;width: 94px;display:none;" type="button" class="enter" value="编辑" />
                    <input id="hidden-save" type="submit" style="float: right;width: 94px;display:none;" class="enter" value="保存" onsubmit="return FormSubmit();" />
                </div>
                <table class="table table-bordered" style="font-size: 13px;margin-top:20px;">
