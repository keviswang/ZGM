<UserControl x:Class="Taizhou.PLE.LawCom.Controls.MapToogleButton"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    d:DesignHeight="300" d:DesignWidth="400">
    
    <Grid x:Name="LayoutRoot" Background="White">
        <Border x:Name="border" BorderBrush="#d6d4d4" BorderThickness="1" Background="#25a0da" Height="40" Width="40">
            <Grid x:Name="grid" Width="38" Height="38" MouseLeftButtonDown="Grid_MouseLeftButtonDown">
                <TextBlock x:Name="textBlock" Text="a" Foreground="White" FontSize="26" FontFamily="/NBZGM.PLE.LawCom;component/Fonts/icomoon.ttf#icomoon" HorizontalAlignment="Center" VerticalAlignment="Center" />
            </Grid>
        </Border>
    </Grid>
</UserControl>
