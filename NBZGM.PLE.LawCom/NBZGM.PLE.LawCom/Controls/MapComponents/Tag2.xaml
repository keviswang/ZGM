<UserControl x:Class="Taizhou.PLE.LawCom.Controls.MapComponents.Tag2"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    d:DesignHeight="300" d:DesignWidth="400">

    <Grid x:Name="LayoutRoot" Background="White">
        <Border BorderBrush="#d6d4d4" BorderThickness="1">
            <StackPanel HorizontalAlignment="Left" VerticalAlignment="Top" Orientation="Horizontal" Height="26" Background="#25a0da">
                <TextBlock x:Name="text" FontSize="14" Margin="6,0,0,0" Foreground="White" VerticalAlignment="Center" />
            </StackPanel>
        </Border>
    </Grid>
</UserControl>
