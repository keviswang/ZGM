@model Taizhou.PLE.Model.TJGHZF
@{
    ViewBag.Title = "GHZFChartAdd";
    Layout = "~/Views/AidDecisionMaking/_LayoutStatisticeManagement.cshtml";
}

<style type="text/css">
    .table th, .table td {
        padding: 3px;
        text-align: center;
        vertical-align: middle;
    }

    .table input {
        width: 90%;
    }
</style>

<script type="text/javascript">
    function ChangeYM() {
        var unitId = $("#UNITID").val();
        var GHZFYear = $("#GHZFYear").val();
        var GHZFMonth = $("#GHZFMonth").val();
        location.href = "/Statistics/GHZFChartAdd?GHZFYear=" + GHZFYear + "&GHZFMonth=" + GHZFMonth;
    }
    $(document).ready(function () {
        var hidGHZFYear = $("#hidGHZFYear").val();
        var hidGHZFMonth = $("#hidGHZFMonth").val();
        $("#GHZFYear").val(hidGHZFYear);
        $("#GHZFMonth").val(hidGHZFMonth);
    });
</script>



<div class="row-fluid">
    <div class="span12" id="tabledata">
        <div class="box box-black" style="margin-top: 0px;">
            <div class="box-title" style="color: white; height: 36px; line-height: 36px; padding-bottom: 0px; padding-top: 0px;">
                <i class="icon-reorder" style="margin-right: 5px;"></i>规划执法统计增加数据               
            </div>
            <div class="box-content">
                <form method="post" enctype="multipart/form-data" action="/Statistics/AddGHZF" id="EventReport">
                    <table class="table table-bordered">
                        <tr>
                            <th style="text-align: center;">统计时间</th>
                            <td colspan="7" style="text-align: left;">
                                <span>
                                    <select id="GHZFYear" name="GHZFYear" onchange="ChangeYM()">
                                        <option value="2014">2014</option>
                                        <option value="2015">2015</option>
                                        <option value="2016">2016</option>
                                    </select></span>
                                <span>
                                    <select id="GHZFMonth" name="GHZFMonth" onchange="ChangeYM()">
                                        <option value="1">一月</option>
                                        <option value="2">二月</option>
                                        <option value="3">三月</option>
                                        <option value="4">四月</option>
                                        <option value="5">五月</option>
                                        <option value="6">六月</option>
                                        <option value="7">七月</option>
                                        <option value="8">八月</option>
                                        <option value="9">九月</option>
                                        <option value="10">十月</option>
                                        <option value="11">十一月</option>
                                        <option value="12">十二月</option>
                                    </select></span>

                                <input type="hidden" id="hidGHZFYear" value="@ViewBag.GHZFYear" />
                                <input type="hidden" id="hidGHZFMonth" value="@ViewBag.GHZFMonth" />
                            </td>
                        </tr>
                        <tr>
                            <td colspan="8" style="font-size: 15px; padding-left: 10px; line-height: 40px; text-align: left;"><strong>案源</strong></td>
                        </tr>
                        <tr>
                            <th style="text-align: center;">巡查发现数</th>
                            <td style="width: 16%">
                                <input type="text" id="AYXCFX" name="AYXCFX" class="small" value="@Model.AYXCFX"/>
                            </td>
                            <th>其他</th>
                            <td style="width: 16%">
                                <input type="text" id="AYOTHER" name="AYOTHER" class="small" value="@Model.AYOTHER" />
                            </td>
                            <td colspan="4">&nbsp;</td>

                        </tr>
                        <tr>
                            <td colspan="8" style="font-size: 15px; padding-left: 10px; line-height: 40px; text-align: left;"><strong>基本统计</strong></td>
                        </tr>
                        <tr>
                            <th>立案数</th>
                            <td style="width: 16%">
                                <input type="text" id="LA" name="LA" class="small" value="@Model.LA" />
                            </td>
                            <th>办结数</th>
                            <td style="width: 16%">
                                <input type="text" id="BJ" name="BJ" class="small" value="@Model.BJ"/>
                            </td>
                            <th>罚款(元)</th>
                            <td style="width: 16%">
                                <input type="text" id="FK" name="FK" class="small" value="@Model.FK" />
                            </td>
                            <th>拆违面积(平方米)</th>
                            <td style="width: 16%">
                                <input type="text" id="WFMJ" name="WFMJ" class="small" value="@Model.WFMJ" />
                            </td>
                        </tr>
                        <tr>
                            <th>没收实物(平方米)</th>
                            <td style="width: 16%">
                                <input type="text" id="MSSW" name="MSSW" class="small" value="@Model.MSSW" />
                            </td>
                            <th>没收违法收入(元)</th>
                            <td style="width: 16%">
                                <input type="text" id="MSWFSR" name="MSWFSR" class="small" value="@Model.MSWFSR" />
                            </td>
                            <td colspan="4">&nbsp;</td>
                        </tr>
                        <tr>
                            <td colspan="8" style="font-size: 15px; padding-left: 10px; line-height: 40px; text-align: left;"><strong>其他</strong></td>
                        </tr>
                        <tr>
                            <th>责令停止建设(件)</th>
                            <td style="width: 16%">
                                <input type="text" id="OZLTZJS" name="OZLTZJS" class="small" value="@Model.OZLTZJS" />
                            </td>
                            <th>定点放样(件)</th>
                            <td style="width: 16%">
                                <input type="text" id="ODDFY" name="ODDFY" class="small" value="@Model.ODDFY" />
                            </td>
                            <th>验收(元)</th>
                            <td style="width: 16%">
                                <input type="text" id="OYS" name="OYS" class="small" value="@Model.OYS" />
                            </td>
                            <th>组织听证(件)</th>
                            <td style="width: 16%">
                                <input type="text" id="OZZTZ" name="OZZTZ" class="small" value="@Model.OZZTZ" />
                            </td>
                        </tr>
                        <tr>
                            <td rowspan="2" colspan="2"><strong>申请法院执行</strong></td>
                            <th>申请(件)</th>
                            <td style="width: 16%">
                                <input type="text" id="OSQFYZXSQ" name="OSQFYZXSQ" class="small" value="@Model.OSQFYZXSQ" />
                            </td>
                            <td rowspan="2" colspan="2"><strong>报请市政府责成</strong></td>
                            <th>申请(件)</th>
                            <td style="width: 16%">
                                <input type="text" id="OBQSZFZCALL" name="OBQSZFZCALL" class="small" value="@Model.OBQSZFZCALL" />
                            </td>
                        </tr>
                        <tr>
                            <th>执结(元)</th>
                            <td style="width: 16%">
                                <input type="text" id="OSQFYZXZJ" name="OSQFYZXZJ" class="small" value="@Model.OSQFYZXZJ" />
                            </td>
                            <th>执结(元)</th>
                            <td style="width: 16%">
                                <input type="text" id="OBQSZFZCZJ" name="OBQSZFZCZJ" class="small" value="@Model.OBQSZFZCZJ" />
                            </td>
                        </tr>

                        <tr>
                            <th>行政复议或诉讼(件)</th>
                            <td style="width: 16%">
                                <input type="text" id="OXZFYSS" name="OXZFYSS" class="small"  value="@Model.OXZFYSS" />
                            </td>
                            <td colspan="6">&nbsp;</td>
                        </tr>
                        <tr>
                            <th>填报人</th>
                            <td style="width: 16%">
                                <input type="text" id="CREATEUSER" name="CREATEUSER" class="small" value="@Model.CREATEUSER" />
                            </td>
                            <th>审核领导</th>
                            <td style="width: 16%">
                                <input type="text" id="CHECKUSSER" name="CHECKUSSER" class="small" value="@Model.CHECKUSSER" />
                            </td>
                            <td colspan="4">&nbsp;</td>
                        </tr>
                    </table>
                    <input type="hidden" id="UNITID" name="UNITID" value="@ViewBag.UNITID" />
                    @if (ViewBag.UNITID == 0)
                    { 
                        <div style="text-align: center; color: red;">对不起，你不符合添加要求！</div>
                    }
                    else
                    {
                        <div class="form-group" style="margin-bottom: 5px; margin-left: 130px;">
                            <div class="col-sm-9 col-lg-10 controls">
                                <input type="submit" class="btn btn-success" value="提　交" onsubmit="return FormSubmit();">
                                <a style="margin-left: 5px;" href="javascript:history.go(-1)" class="btn goBack">返　回</a>
                            </div>
                        </div>
                    }
                </form>
            </div>
        </div>
    </div>
</div>
