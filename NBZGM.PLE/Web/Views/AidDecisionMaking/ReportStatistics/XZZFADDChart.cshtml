@model Taizhou.PLE.Model.XZZFTABLIST
@{
    ViewBag.Title = "XZZFADDChart";
    Layout = "~/Views/AidDecisionMaking/_LayoutStatisticeManagement.cshtml";
}

<style type="text/css">
    .table th, .table td {
        padding: 3px;
        text-align: center;
        vertical-align: middle;
    }

    .table input {
        width: 90%;
    }
</style>

<script type="text/javascript">
    //$("#CHUFZL").change(function () {
    //    var CHUFZLXL = $("#CHUFZL").val();
    //    var XZZFYear = $("#XZZFYear").val();
    //    var XZZFMonth = $("#XZZFMonth").val();
    //    var QuestionDLID = $('#QuestionDLID').val();
    //    $.ajax({
    //        type: "POST",
    //        url: "/Statistics/XZZFChartAdd",
    //        data: { XZZFYear:XZZFYear,XZZFMonth:XZZFMonth,CHUFZLXL:CHUFZLXL },
    //        cache: false,
    //        success: function (data) {
    //        }
    //    });
    //});
    function ChangeYM() {
        var CHUFZLXL = $("#CHUFZL").val();
        var XZZFYear = $("#XZZFYear").val();
        var XZZFMonth = $("#XZZFMonth").val();
        var UnitNameID = $("#Units").val();
        alert(UnitNameID);
        location.href = "/Statistics/XZZFChartAdd?XZZFYear=" + XZZFYear + "&XZZFMonth=" + XZZFMonth + "&CHUFZLXL=" + CHUFZLXL + "&UnitNameID=" + UnitNameID;
    }
    $(document).ready(function () {
        //获得后台传过来赋给页面隐藏控件的值
        var hidXZZFYear = $("#hidXZZFYear").val();
        var hidXZZFMonth = $("#hidXZZFMonth").val();
        //赋给页面下拉框的默认值
        $("#XZZFYear").val(hidXZZFYear);
        $("#XZZFMonth").val(hidXZZFMonth);
    });
</script>

<div class="row-fluid">
    <div class="span12" id="tabledata">
        <div class="box box-black" style="margin: 5px;">
            <div class="box-title" style="color: white; height: 36px; line-height: 36px; padding-bottom: 0px; padding-top: 0px;">
                <i class="icon-reorder" style="margin-right: 5px;"></i>行政执法数据添加
            </div>
            <div class="box-content">
                <form method="post" enctype="multipart/form-data" action="/Statistics/AddXZZFTabList" id="EventReport">
                    <table class="table table-bordered">
                        <tr>
                            <th style="text-align: center;">处罚种类：</th>
                            <td colspan="3" style="width: 16%; text-align: left">
                                @Html.DropDownList("CHUFZL", ViewBag.CHUFZL as IEnumerable<SelectListItem>)
                            </td>
                            <th style="text-align: center;">部门：</th>
                            <td colspan="3" style="width: 16%; text-align: left">
                                @Html.DropDownList("Units", ViewBag.UnitList as IEnumerable<SelectListItem>)
                            </td>
                        </tr>
                        <tr>
                            <th style="text-align: center;">统计时间</th>
                            <td colspan="7" style="text-align: left;">
                                <span>
                                    <select id="XZZFYear" name="XZZFYear" onchange="ChangeYM()">
                                        <option value="2014">2014</option>
                                        <option value="2015">2015</option>
                                        <option value="2016">2016</option>
                                    </select></span>
                                <span>
                                    <select id="XZZFMonth" name="XZZFMonth" onchange="ChangeYM()">
                                        <option value="1">一月</option>
                                        <option value="2">二月</option>
                                        <option value="3">三月</option>
                                        <option value="4">四月</option>
                                        <option value="5">五月</option>
                                        <option value="6">六月</option>
                                        <option value="7">七月</option>
                                        <option value="8">八月</option>
                                        <option value="9">九月</option>
                                        <option value="10">十月</option>
                                        <option value="11">十一月</option>
                                        <option value="12">十二月</option>
                                    </select></span>

                                <input type="hidden" id="hidXZZFYear" value="@ViewBag.XZZFYear" />
                                <input type="hidden" id="hidXZZFMonth" value="@ViewBag.XZZFMonth" />
                            </td>
                        </tr>
                        <tr>
                            <td colspan="8" style="font-size: 15px; padding-left: 10px; line-height: 40px; text-align: left;"><strong>案源</strong></td>
                        </tr>
                        <tr>
                            <th style="text-align: center;">巡查发现数</th>
                            <td style="width: 16%">
                                <input type="text" id="AYXCFX" name="AYXCFX" class="small" value="@Model.AYXCFX" />
                            </td>
                            <th>其他</th>
                            <td style="width: 16%">
                                <input type="text" id="ANYOTHER" name="ANYOTHER" class="small" value="@Model.ANYOTHER" />
                            </td>
                            <td colspan="4">&nbsp;</td>

                        </tr>
                        <tr>
                            <td colspan="8" style="font-size: 15px; padding-left: 10px; line-height: 40px; text-align: left;"><strong>简易程序</strong></td>
                        </tr>
                        <tr>
                            <td rowspan="2" colspan="2"><strong>罚款</strong></td>
                            <th>件</th>
                            <td style="width: 16%">
                                <input type="text" id="SIMPLEFKJ" name="SIMPLEFKJ" class="small" value="@Model.SIMPLEFKJ" />
                            </td>
                            <td colspan="4">&nbsp;</td>
                        </tr>
                        <tr>
                            <th>元</th>
                            <td style="width: 16%">
                                <input type="text" id="SIMPLEFKY" name="SIMPLEFKY" class="small" value="@Model.SIMPLEFKY" />
                            </td>
                            <td colspan="4">&nbsp;</td>
                        </tr>
                        <tr>
                            <td colspan="8" style="font-size: 15px; padding-left: 10px; line-height: 40px; text-align: left;"><strong>一般程序</strong></td>
                        </tr>
                        <tr>
                            <th>立案件数</th>
                            <td style="width: 16%">
                                <input type="text" id="CASELA" name="CASELA" class="small" value="@Model.CASELA" />
                            </td>
                            <th>办结件数</th>
                            <td style="width: 16%">
                                <input type="text" id="CASEBJ" name="CASEBJ" class="small" value="@Model.CASEBJ" />
                            </td>
                            <th>罚款（元）</th>
                            <td style="width: 16%">
                                <input type="text" id="CASEFAKY" name="CASEFAKY" class="small" value="@Model.CASEFAKY" />
                            </td>
                            <th>没收违法所得（元）</th>
                            <td style="width: 16%">
                                <input type="text" id="CASEMSWFSDY" name="CASEMSWFSDY" class="small" value="@Model.CASEMSWFSDY" />
                            </td>
                        </tr>
                        <tr>
                            <th>没收违法财物（元）</th>
                            <td style="width: 16%">
                                <input type="text" id="CASEMSWFCWY" name="CASEMSWFCWY" class="small" value="@Model.CASEMSWFCWY" />
                            </td>
                            <th>强制措施（件）</th>
                            <td style="width: 16%">
                                <input type="text" id="CASEQZCSJ" name="CASEQZCSJ" class="small" value="@Model.CASEQZCSJ" />
                            </td>
                            <th>责令停产停业</th>
                            <td style="width: 16%">
                                <input type="text" id="CASEZLTYJ" name="CASEZLTYJ" class="small" value="@Model.CASEZLTYJ" />
                            </td>
                            <th>其他处罚（件）</th>
                            <td style="width: 16%">
                                <input type="text" id="CASEOTHER" name="CASEOTHER" class="small" value="@Model.CASEOTHER" />
                            </td>
                        </tr>
                        <tr>
                        </tr>
                        <tr>
                            <th>其中组织听证（件）</th>
                            <td style="width: 16%">
                                <input type="text" id="CASEZZTZ" name="CASEZZTZ" class="small" value="@Model.CASEZZTZ" />
                            </td>
                            <th>申请法院执行（件）</th>
                            <td style="width: 16%">
                                <input type="text" id="CASESQFYZX" name="CASESQFYZX" class="small" value="@Model.CASESQFYZX" />
                            </td>
                            <td colspan="4">&nbsp;</td>
                        </tr>
                        <tr>
                            <th>填报人</th>
                            <td style="width: 16%">
                                <input type="text" id="TBUSER" name="TBUSER" class="small" value="@Model.TBUSER" />
                            </td>
                            <th>审核领导</th>
                            <td style="width: 16%">
                                <input type="text" id="SHUSER" name="SHUSER" class="small" value="@Model.SHUSER" />
                            </td>
                            <td colspan="4">&nbsp;</td>
                        </tr>
                    </table>
                    <div class="form-group" style="margin-bottom: 5px; margin-left: 130px;">
                        <div class="col-sm-9 col-lg-10 controls">
                            <input type="submit" class="btn btn-success" value="提　交" onsubmit="return FormSubmit();">
                            <a style="margin-left: 5px;" href="javascript:history.go(-1)" class="btn goBack">返　回</a>
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>
