@{
    Taizhou.PLE.Model.CaseWorkflowModels.TotalForm wmCaseForm = ViewBag.TotalForm;
    Taizhou.PLE.Model.CaseWorkflowModels.Form101 wmForm1 = wmCaseForm.Form101;
    Taizhou.PLE.Model.CaseWorkflowModels.Form102 wmForm2 = wmCaseForm.Form102;
    Taizhou.PLE.Model.CaseWorkflowModels.Form103 wmForm3 = wmCaseForm.Form103;
}

<div class="form">
    <div class="title">
        立案审批表
    </div>
    <table class="workflow" border="0" cellspacing="0" cellpadding="0" style="width: 700px; border-collapse: collapse; margin: 0 auto; margin-top: 20px;">
        <tr>
            <th colspan="2">案　　由</th>
            <td colspan="9">
                <input name="ay" type="text" id="ay" disabled="disabled" value="@wmForm1.AY" />
            </td>
        </tr>
        <tr>
            <th colspan="2">发案地点</th>
            <td colspan="9">
                <input name="fadd" type="text" id="fadd" disabled="disabled" value="@wmForm1.FADD" />
            </td>
        </tr>
        <tr>
            <th colspan="2">发案时间</th>
            <td colspan="3">
                <input type="text" name="fass" id="fass" disabled="disabled" value="@wmForm1.FASJ" />
            </td>
            <th colspan="2">案件来源</th>
            <th colspan="4">
                <select name="ajly" id="ajly" disabled="disabled">
                    <option value="@wmForm1.AJLYID">@wmForm1.AJLYName</option>
                </select></th>
        </tr>
        <tr>
            <th colspan="2">是否立案</th>
            <td colspan="3">
                @{
                    string laChecked3 = wmForm1.SFLA == "la" ? "checked=\"checked\"" : "";
                    string zfChecked3 = wmForm1.SFLA == "zf" ? "checked=\"checked\"" : "";
                }
                <input type="radio" @laChecked3 value="la" disabled="disabled" />立案
                <input type="radio" @zfChecked3 value="zf" disabled="disabled" />作废
            </td>
            <th colspan="2">承办单位</th>
            <th colspan="4" id="td_CBDWID" class="control-group">
                <select disabled="disabled">
                    <option value="@wmForm1.CBDWID">@wmForm1.CBDWName</option>
                </select>)
            </th>
        </tr>
        <tr>
            <th colspan="2">是否为重大案件</th>
            @{
                string SWZDAN3 = wmForm1.SFWZDAN == "s" ? "checked=\"checked\"" : "";
                string FWZDAN3 = wmForm1.SFWZDAN == "f" ? "checked=\"checked\"" : "";
            }
            <td colspan="9">
                <input type="radio" @SWZDAN3 value="la" disabled="disabled" />是
                <input type="radio" @FWZDAN3 value="la" disabled="disabled" />否
            </td>
        </tr>
        <tr>
            <th style="width: 42px;" rowspan="6">当事<br />
                人基<br />
                本情<br />
                况</th>
            <th colspan="2">当事人类型</th>
            @{
                string dw3 = wmForm1.DSRLX == "dw" ? "checked=\"checked\"" : "";
                string gr3 = wmForm1.DSRLX == "gr" ? "checked=\"checked\"" : "";
            }
            <td colspan="8">
                <input type="radio" @dw3 value="la" disabled="disabled" />单位
                <input type="radio" @gr3 value="la" disabled="disabled" />个人
            </td>
        </tr>
        <tr>
            <th style="width: 42px;" rowspan="2">单位</th>
            <th style="width: 42px;">名称</th>
            <td colspan="4">
                <input type="text" id="mc" name="mc"  disabled="disabled" value="@wmForm1.OrgForm.MC" />
            </td>
            <th colspan="2">组织机构<br />
                代码证编号</th>
            <td colspan="2">
                <input type="text" id="zzjgdmzbh" name="zzjgdmzbh"  disabled="disabled" value="@wmForm1.OrgForm.ZZJGDMZBH" />
            </td>
        </tr>
        <tr>
            <th colspan="2">法定代表人<br />
                （负责人）姓名</th>
            <td colspan="3">
                <input type="text" id="fddbrxm" name="fddbrxm"  disabled="disabled" value="@wmForm1.OrgForm.FDDBRXM" />
            </td>
            <th colspan="2">职务</th>
            <td colspan="2">
                <input type="text" id="zw" name="zw"  disabled="disabled" value="@wmForm1.OrgForm.ZW" />
            </td>
        </tr>
        <tr>
            <th style="width: 42px;" rowspan="2">个人</th>
            <th style="width: 42px;">姓名</th>
            <td style="width: 92px;">
                <input type="text" id="xm" name="xm" category="gr" disabled="disabled" value="@wmForm1.PersonForm.XM" />
            </td>
            <th style="width: 42px;">性别</th>
            <td style="width: 72px;">
                <select name="xb" id="xb" category="gr" disabled="disabled">
                    <option value="@wmForm1.PersonForm.XB">@wmForm1.PersonForm.XB</option>
                </select></td>
            <th style="width: 52px;">出生<br />
                年月</th>
            <td style="width: 102px;" colspan="2">
                <input type="text" id="csny" name="csny" category="gr" disabled="disabled" value="@wmForm1.PersonForm.CSNY" />
            </td>
            <th style="width: 42px;">民族</th>
            <td style="width: 92px;">
                <input type="text" id="mz" name="mz" category="gr" disabled="disabled" value="@wmForm1.PersonForm.MZ" />
            </td>
        </tr>
        <tr>
            <th style="width: 42px;">身份<br />
                证号</th>
            <td colspan="2">
                <input type="text" id="sfzh" name="sfzh" category="gr" disabled="disabled" value="@wmForm1.PersonForm.SFZH" />
            </td>
            <th style="width: 72px;">工作单位</th>
            <td colspan="5">
                <input type="text" id="gzdw" name="gzdw" category="gr" disabled="disabled" value="@wmForm1.PersonForm.GZDW" />
            </td>
        </tr>
        <tr>
            <th colspan="2">住所地<br />
                （住址）</th>
            <td colspan="5">
                <input type="text" id="zsd" name="zsd" disabled="disabled" value="@wmForm1.ZSD" /></td>
            <th style="width: 42px;">联系<br />
                电话</th>
            <td colspan="2">
                <input type="text" id="lxdh" name="lxdh" disabled="disabled" value="@wmForm1.LXDH" />
            </td>
        </tr>
        <tr>
            <th style="width: 42px;">案情<br />
                摘要</th>
            <td colspan="10">
                <textarea id="aqzy" name="aqzy" disabled="disabled">@wmForm1.AQZY</textarea>
            </td>
        </tr>
        <tr>
            <th style="width: 42px;">违法<br />
                行为</th>
            <td colspan="10" id="wfxw">
                <div>
                    违法行为代码：@wmForm1.IllegalForm.Code
                </div>

                <div>
                    违法行为:@wmForm1.IllegalForm.Name
                </div>
                <div>
                    适用的违则：@wmForm1.IllegalForm.WeiZe
                </div>
                <div>
                    适用的罚则：@wmForm1.IllegalForm.FaZe
                </div>
                <div>
                    适用的处罚：@wmForm1.IllegalForm.ChuFa
                </div>

            </td>
        </tr>
        <tr>
            <th style="width: 42px;">立案<br />
                理由</th>
            <td colspan="10">
                <textarea id="laly" name="laly" disabled="disabled">@wmForm1.LALY</textarea>
            </td>
        </tr>
        <tr>
            <th style="width: 42px;">拟办<br />
                意见</th>
            <td colspan="10">
                <textarea id="nbyj" name="nbyj" disabled="disabled">@wmForm1.NBYJ</textarea>
            </td>
        </tr>
         <tr>
            <th style="width: 42px;">拟办<br />
                队员</th>
            <td colspan="5">
                <select name="NBDYID1" id="NBDYID1" disabled="disabled">
                    <option value="@wmForm1.NBDYID1">@wmForm1.NBDYNAME1</option>
                </select>
            </td>
            <td colspan="5">
                <select name="NBDYID2" id="NBDYID2" disabled="disabled">
                    <option value="@wmForm1.NBDYID2">@wmForm1.NBDYNAME2</option>
                </select>
            </td>
        </tr>
        <tr>
            <th style="width: 42px;">办案<br />
                单位<br />
                意见</th>
            <td colspan="10">
                <textarea id="bndwyj" name="bndwyj" disabled="disabled">@wmForm2.LDSPYJ</textarea>
            </td>
        </tr>
        @if (!string.IsNullOrWhiteSpace(wmForm3.THYJ))
        {
            <tr>
                <th style="width: 42px;">退回<br />
                    意见</th>
                <td colspan="10">
                    @Html.TextAreaFor(model => wmForm3.THYJ, new { disabled = "disabled" })
                </td>
            </tr>
        }
        <tr>
            <th style="width: 42px;">主办<br />
                人员</th>
            <td colspan="10">
                <select name="zbdy" id="zbdy" disabled="disabled">
                    <option value="@wmForm2.ZBDY.UserID">@wmForm2.ZBDY.UserName</option>
                </select>
            </td>

        </tr>
        <tr>
            <th style="width: 42px;">协办<br />
                人员</th>
            <td colspan="10">
                <select name="xbdy" id="xbdy" disabled="disabled">
                    <option value="@wmForm2.XBDY.UserID">@wmForm2.XBDY.UserName</option>
                </select>
            </td>

        </tr>
        <tr>
            <th style="width: 42px;">法制<br />
                机构<br />
                意见</th>
            <td colspan="10">
                @Html.TextArea("fzjgyj", wmForm3.FZJGYJ, new { disabled = "disabled" })
            </td>
        </tr>
        <tr>
            <th>处理人</th>
            <td colspan="4">@Html.TextBox("clr", wmForm3.ProcessUser.UserName, new { disabled = "disabled" })</td>
            <th colspan="2">处理时间</th>
            <td colspan="4">@Html.TextBox("clsj", wmForm3.ProcessTime != null ? wmForm3.ProcessTime.Value.ToString("yyyy-MM-dd HH:mm:ss") : "", new { disabled = "disabled" })</td>
        </tr>
        <tr style="height: 0px; line-height: 0px;">
            <td style="width: 42px; border: none; padding: 0px;"></td>
            <td style="width: 42px; border: none; padding: 0px;"></td>
            <td style="width: 42px; border: none; padding: 0px;"></td>
            <td style="width: 92px; border: none; padding: 0px;"></td>
            <td style="width: 42px; border: none; padding: 0px;"></td>
            <td style="width: 72px; border: none; padding: 0px;"></td>
            <td style="width: 52px; border: none; padding: 0px;"></td>
            <td style="width: 52px; border: none; padding: 0px;"></td>
            <td style="width: 42px; border: none; padding: 0px;"></td>
            <td style="width: 42px; border: none; padding: 0px;"></td>
            <td style="border: none; padding: 0px;">&nbsp;</td>
        </tr>
    </table>
</div>

