@{
    Taizhou.PLE.Model.CaseWorkflowModels.TotalForm wmCaseForm = ViewBag.TotalForm;
    Taizhou.PLE.Model.CaseWorkflowModels.Form101 wmForm1 = wmCaseForm.Form101;
    Taizhou.PLE.Model.CaseWorkflowModels.Form105 wmForm5 = wmCaseForm.Form105;
    Taizhou.PLE.Model.CaseWorkflowModels.Form106 wmForm6 = wmCaseForm.Form106;
}

<div class="form">
    <div class="title">
        协办队员确认调查取证
    </div>

    <table class="workflow" border="0" cellspacing="0" cellpadding="0" style="width: 700px; border-collapse: collapse; margin: 0 auto; margin-top: 20px;">
        <tr>
            <th colspan="2">调查取证说明</th>
            <td colspan="9">
                <textarea id="dcqzsm" name="dcqzsm" disabled="disabled">@wmForm5.Description</textarea>
            </td>
        </tr>

        <tr>
            <th colspan="2">确认意见</th>
            <td colspan="9">
                @Html.TextArea("qryj", wmForm6.QRYJ, new { disabled = "disabled" })
            </td>
        </tr>
        <tr>
            <th>处理人</th>
            <td colspan="4">@Html.TextBox("clr", wmForm6.ProcessUser.UserName, new { disabled = "disabled" })</td>
            <th colspan="2">处理时间</th>
            <td colspan="4">@Html.TextBox("clsj", wmForm6.ProcessTime != null ? wmForm6.ProcessTime.Value.ToString("yyyy-MM-dd HH:mm:ss") : "", new { disabled = "disabled" })</td>
        </tr>
        <tr style="height: 0px; line-height: 0px;">
            <td style="width: 42px; border: none; padding: 0px;"></td>
            <td style="width: 42px; border: none; padding: 0px;"></td>
            <td style="width: 42px; border: none; padding: 0px;"></td>
            <td style="width: 92px; border: none; padding: 0px;"></td>
            <td style="width: 42px; border: none; padding: 0px;"></td>
            <td style="width: 72px; border: none; padding: 0px;"></td>
            <td style="width: 52px; border: none; padding: 0px;"></td>
            <td style="width: 52px; border: none; padding: 0px;"></td>
            <td style="width: 42px; border: none; padding: 0px;"></td>
            <td style="width: 42px; border: none; padding: 0px;"></td>
            <td style="border: none; padding: 0px;">&nbsp;</td>
        </tr>
    </table>
</div>
