                                                Grid.ColumnSpan="2"
                                                Grid.Row="1">
                                            <Border BorderBrush="{StaticResource CalendarInnerBorder}"
                                                    BorderThickness="1 0 1 1">
                                                <telerik:RadToggleButton x:Name="PART_Close"
                                                                         HorizontalAlignment="Right"
                                                                         IsThreeState="False"
                                                                         IsBackgroundVisible="False"
                                                                         IsChecked="{Binding IsDropDownOpen, Mode=TwoWay, RelativeSource={RelativeSource TemplatedParent}}"
                                                                         Margin="2"
                                                                         Padding="8 3"
                                                                         telerik:LocalizationManager.ResourceKey="Close"
                                                                         Style="{StaticResource CloseButtonStyle}" />
                                            </Border>
                                        </Border>
                                    </Grid>
                                </telerik:RadDropDownButton.DropDownContent>
                                <telerik:RadDropDownButton.Template>
                                    <ControlTemplate TargetType="telerik:RadDropDownButton">
                                        <Grid>
                                            <ContentPresenter HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                                                              Margin="{TemplateBinding Padding}"
                                                              VerticalAlignment="{TemplateBinding VerticalContentAlignment}" />
                                            <telerik:Popup x:Name="DropDownPopup">
                                                <ContentPresenter x:Name="DropDownPopupContent"
                                                                  ContentTemplate="{TemplateBinding DropDownContentTemplate}"
                                                                  Content="{TemplateBinding DropDownContent}"
                                                                  DataContext="{Binding DataContext, RelativeSource={RelativeSource TemplatedParent}}" />
                                            </telerik:Popup>
                                        </Grid>
                                    </ControlTemplate>
                                </telerik:RadDropDownButton.Template>
                                <Grid Margin="0,0,0,0">
                                    <Chromes:ButtonChrome x:Name="ButtonChrome"
                                                          BorderBrush="{TemplateBinding BorderBrush}"
                                                          BorderThickness="{TemplateBinding BorderThickness}"
                                                          CornerRadius="{StaticResource SplitButton_RightPartCornerRadius}"
                                                          RenderMouseOver="{Binding IsMouseOver, ElementName=PART_DropDownButton}"
                                                          
                                                          Background="{x:Null}" />
                                    <!--telerik:StyleManager.Theme="{StaticResource Theme}"-->
                                    <ContentControl x:Name="DropDownIcon"
                                                    Height="22"
                                                    VerticalAlignment="Bottom"
                                                    Foreground="{StaticResource ButtonIconForeground_Normal}"
                                                    IsTabStop="False"
                                                    Template="{StaticResource CalendarIcon}" />
                                </Grid>
                            </telerik:RadDropDownButton>
                            <Chromes:ValidationTooltip x:Name="ValidationTooltip"
                                                       Grid.ColumnSpan="2"
                                                       TooltipPlacementTarget="{Binding RelativeSource={RelativeSource TemplatedParent}}"
                                                      
                                                       TooltipContent="{Binding (Validation.Errors), RelativeSource={RelativeSource TemplatedParent}}"
                                                       Visibility="Collapsed" />
                            <!--telerik:StyleManager.Theme="{StaticResource Theme}"-->
                            <telerik:Popup IsOpen="{Binding IsTooltipOpen, Mode=TwoWay, RelativeSource={RelativeSource TemplatedParent}}"
                                           Owner="{Binding RelativeSource={RelativeSource TemplatedParent}}"
                                           Placement="Top">
                                <telerik:PopupPlacement.PopupPlacement>
                                    <telerik:PopupPlacement PlacementTarget="{Binding ElementName=PART_DateTimeInput}" />
                                </telerik:PopupPlacement.PopupPlacement>
                                <ContentPresenter ContentTemplate="{TemplateBinding TooltipTemplate}"
                                                  Content="{TemplateBinding TooltipContent}" />
                            </telerik:Popup>
                        </Grid>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>




    </UserControl.Resources>

    <Grid x:Name="LayoutRoot">
        <!--<Grid.Background>
            <ImageBrush ImageSource="/ZGM.WUA;component/TrackPlayback/TrackPlaybackBackground.png" Stretch="Fill"></ImageBrush>
        </Grid.Background>-->
        <Grid.RowDefinitions>
            <RowDefinition />            
        </Grid.RowDefinitions>
        <Image Source="/ZGM.WUA;component/Images/TrackPlayback/TrackPlaybackBackground.png" Stretch="Fill"/>
        <telerik:RadBusyIndicator x:Name="BusyIndicator"
                                  Grid.Row="0"
                                  Grid.RowSpan="2"
                                  IsBusy="False"
                                  BusyContent="加载中..."
                                  IsIndeterminate="True"
                                  telerik:StyleManager.Theme="Expression_Dark" />
        <Border BorderBrush="#b0b6bd"
                
                BorderThickness="0,0,0,1"
                Margin="0,0,0,0" />

        <!-- 搜索 -->
        <Grid>
            <Grid.RowDefinitions>
                <RowDefinition Height="45*" />
                <RowDefinition Height="55*" />
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="70*" />
                <ColumnDefinition Width="200*" />
                <ColumnDefinition Width="70*" />
                <ColumnDefinition Width="200*" />
                <ColumnDefinition Width="100*" />
            </Grid.ColumnDefinitions>

            <TextBlock Text="开始时间"
                       VerticalAlignment="Center"
                       HorizontalAlignment="Right"
                       TextOptions.TextHintingMode="Fixed"
                       Style="{StaticResource TextBlockStyle}"
                       Grid.Row="0"
                       Height="16"
                       Margin="0,15,0,14"
                       Width="48" />
            <TextBlock Text="结束时间"
                       VerticalAlignment="Center"
                       HorizontalAlignment="Right"
                       TextOptions.TextHintingMode="Fixed"
                       Style="{StaticResource TextBlockStyle}"
                       Grid.Row="0"
                       Grid.Column="2"
                       Height="16"
                       Margin="0,13,0,12"
                       Width="48" />
            <TextBlock Text="回放速度"
                       VerticalAlignment="Center"
                       HorizontalAlignment="Right"
                       TextOptions.TextHintingMode="Fixed"
                       Style="{StaticResource TextBlockStyle}"
                       Grid.Row="1"
                       Height="16"
                       Margin="0,17,0,16"
                       Width="48" />
            <telerik:RadDateTimePicker x:Name="StartTimeRDTP"
                                       Width="160"
                                       Background="#2c7dc0"                                       
                                       InputMode="DateTimePicker"
                                       Culture="zh-CN"
                                       HorizontalAlignment="Left"
                                       Foreground="White"
                                       Margin="10,11,0,10"
                                        BorderBrush="White"
                                       BorderThickness="1" 
                                       Grid.Column="1"
                                       Style="{StaticResource StartTimePickerStyle}" />
            <telerik:RadDateTimePicker x:Name="EndTimeRDTP"
                                       Width="160"
                                       InputMode="DateTimePicker"
                                       Culture="zh-CN"
                                       HorizontalAlignment="Left"
                                       Foreground="White"
                                       Margin="10,9,0,9"
                                       Grid.Column="3"
                                       Background="#2c7dc0"
                                       BorderBrush="White"
                                       BorderThickness="1"                                       
                                       Style="{StaticResource StartTimePickerStyle}"
                                       Grid.Row="0" />
            <TextBlock
                Grid.Column="1"
                Grid.Row="1"
                Text="慢"
                Foreground="white"
                Margin="5"></TextBlock>
            <TextBlock
                Grid.Column="3"
                Grid.Row="1"
                Text="快"
                 Foreground="white"
                Margin="5"
                FlowDirection="RightToLeft"></TextBlock>
            <Slider x:Name="SpeedSlider"
                    Grid.ColumnSpan="3"
                    Grid.Column="1"
                    HorizontalAlignment="Left"
                    Margin="10,16,0,15"
                    Grid.Row="1"
                    VerticalAlignment="Center"
                    Width="430"
                    Maximum="45"
                    Value="2"
                    ValueChanged="SpeedSlider_ValueChanged"
                    Style="{StaticResource SpeedSliderStyle}"
                    Height="18"
                    BorderThickness="0,1" />
            
            <Grid Grid.Row="1"
                Grid.Column="4">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="1*"></ColumnDefinition>
                    <ColumnDefinition Width="1*"></ColumnDefinition>
                   
                </Grid.ColumnDefinitions>
                <Button Grid.Column="0"
                        VerticalAlignment="Center"
                        Width="35"
                        Height="35"
                        Cursor="Hand"
                        Style="{StaticResource PlayButtonStyle}"
                        BorderThickness="0"
                        HorizontalAlignment="Left"
                        x:Name="PauseBtn"
                        Click="PauseBtn_Click">
                    <Grid>
                        <Rectangle Width="3"
                                   Height="15"
                                   Fill="White"
                                   VerticalAlignment="Center"
                                   HorizontalAlignment="Center"
                                   Margin="-5,0,3,0" />

                        <Rectangle Width="3"
                                   Height="15"
                                   Fill="White"
                                   VerticalAlignment="Center"
                                   HorizontalAlignment="Center"
                                   Margin="4,0,-2,0" />
                    </Grid>
                </Button>
                <Button Grid.Column="0"
                        Visibility="Visible"
                        VerticalAlignment="Center"
                        Width="35"
                        Height="35"
                        BorderThickness="0"
                        Cursor="Hand"
                        HorizontalAlignment="Left"
                        x:Name="PlayBtn"
                        Click="PlayBtn_Click"
                        Margin="0,4"
                        Style="{StaticResource PlayButtonStyle}">
                    <Polygon Points="0,0 15,8 0,15"
                             Width="15"
                             Height="15"
                             Fill="White" />
                </Button>
                <Button Grid.Column="1"
                        Grid.Row="3"
                        VerticalAlignment="Center"
                        Width="35"
                        Height="35"
                        Cursor="Hand"
                        BorderThickness="0"
                        HorizontalAlignment="Left"
                        Style="{StaticResource PlayButtonStyle}"
                        Click="Button_Click">
                    <Rectangle Width="15"
                               Height="15"
                               Fill="White" />
                </Button>
            </Grid>
        </Grid>
    </Grid>   
</UserControl>
